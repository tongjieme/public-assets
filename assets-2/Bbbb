import {useState} from "react";
import LS from "../utils/LS.js";
import UseReactCacheQuery from "../Comp/UseReactCacheQuery.jsx";

export default function (){
    let _JSON = UseReactCacheQuery("JSON_PLACEHOLDER", function () {
        return fetch('https://jsonplaceholder.typicode.com/todos/1')
            .then(response => response.json())
    }, 10)
    return <>
        {
            JSON.stringify(_JSON.CurrentValue)
        }
        {
            _JSON.Loading && <span>Loading</span>
        }

        <div onClick={e => _JSON.reload()}>Home</div>
    </>
}
