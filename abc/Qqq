function getNonCompletedFunds(allFunds, completedFunds) {
  let allFundsArray = allFunds.split(",");
  let completedFundsArray = completedFunds.split(",");
  
  let nonCompletedFundsArray = allFundsArray.filter(fund => {
    return completedFundsArray.indexOf(fund) === -1;
  });
  
  return nonCompletedFundsArray.join(",");
}
