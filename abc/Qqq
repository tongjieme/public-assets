import {useState} from "react";

export default function ({children, getValue, setValue, width}){
    let _getValue = getValue || (() => {})
    let _setValue = setValue || (() => {})
    const [Width, setWidth] = useState(width)
    return <div style={{width: Width + "px"}}>
        {children}
        <input type="number" value={Width} onChange={e => {
            console.log(Width)
            setWidth(e.target.value)
        }}/>
    </div>
}
